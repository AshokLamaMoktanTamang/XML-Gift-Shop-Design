                <heading_link>About us</heading_link>
                <heading_link>Services</heading_link>
                <heading_link>Contact us</heading_link>
                <heading_link>Checkout</heading_link>
            </heading_link_container>

            <!--     
                making the shortcut navigation
                containing basket and profile
                with the dropdown contents
             -->
            <heading_info>

                <!-- making the gift card basket with dropdown contents -->
                <basket>
                    <basket_logo_container>
                        <basket_logo/>
                        <basket_total>3</basket_total>
                    </basket_logo_container>
    
                    <basket_drop_content_container>
                        <basket_drop_content>
                            <basket_item>
                                <cart_image cartProduct="myntra" />
